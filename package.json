{
  "name": "hikidashi",
  "version": "0.0.1",
  "private": false,
  "repository": "https://github.com/kaihodev/hikidashi",
  "author": "KaihÅ Dev",
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./safe": "./dist/safe.js",
    "./unsafe": "./dist/unsafe.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:main": "node esbuild.config.js && tsc",
    "build:test": "node esbuild.test.config.js && tsc",
    "build:mjs": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "test": "jest",
    "test:coverage": "jest --coverage=true",
    "bench": "sh run-all-benchmarks.sh",
    "prepack": "run-s \"build:?(main|mjs)\"",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.33",
    "@types/ramda": "^0.27.40",
    "@types/underscore": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "benchmark": "^2.1.4",
    "benri": "^0.0.0-alpha-0",
    "esbuild": "^0.11.11",
    "eslint": "^7.21.0",
    "eslint-config-strict": "https://github.com/kaihodev/eslint-config-strict",
    "gen-esm-wrapper": "^1.1.1",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.27.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "underscore": "^1.13.1"
  },
  "browserslist": [
    "> 0%"
  ]
}
